{
  "name": "pico-engine-next",
  "version": "0.49.2",
  "description": "An implementation of the pico-engine targetting node.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Picolab/pico-engine.git"
  },
  "author": "Picolab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Picolab/pico-engine/issues"
  },
  "homepage": "https://github.com/Picolab/pico-engine#readme",
  "keywords": [
    "pico-engine",
    "krl",
    "KRE",
    "IoT"
  ],
  "files": [
    "bin",
    "www",
    "src",
    "dist"
  ],
  "main": "dist/index.js",
  "bin": {
    "pico-engine": "bin/pico-engine"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf dist && tsc",
    "start": "npm run dev -s",
    "dev": "scriptsp -r dev:tsc dev:api",
    "dev:tsc": "tsc -w --preserveWatchOutput",
    "dev:api": "onchange -i -k dist/ -- node dist/cli.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "home-dir": "^1.0.0",
    "krl-compiler": "^0.49.2",
    "krl-stdlib": "^0.49.2",
    "leveldown": "^5.0.2",
    "make-dir": "^3.0.0",
    "pico-framework": "^0.1.1",
    "select-when": "^0.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.43",
    "@types/levelup": "^3.1.0",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.11.5",
    "onchange": "^5.2.0",
    "scriptsp": "^1.1.1",
    "typescript": "^3.3.4000"
  }
}
