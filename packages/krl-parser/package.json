{
  "name": "krl-parser",
  "version": "0.51.1",
  "description": "Parse KRL source code into an AST",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf dist && tsc",
    "start": "ava -w",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Picolab/pico-engine.git"
  },
  "keywords": [
    "pico-engine",
    "krl",
    "parser"
  ],
  "author": "Picolab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Picolab/pico-engine/issues"
  },
  "homepage": "https://github.com/Picolab/pico-engine#readme",
  "devDependencies": {
    "ava": "^2.4.0",
    "diff-lines": "^1.1.0",
    "faucet": "0.0.1",
    "lodash": "^4.17.4",
    "onchange": "^6.0.0",
    "phonetic": "^0.1.1",
    "standard": "^12.0.1",
    "tape": "^4.6.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  },
  "standard": {
    "ignore": [
      "grammar.js"
    ]
  },
  "dependencies": {
    "excerpt-at-line-col": "^1.0.0",
    "line-column": "^1.0.2",
    "nearley": "2.16.0"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**/*"
    ],
    "helpers": [
      "test/helpers/**/*"
    ]
  }
}
