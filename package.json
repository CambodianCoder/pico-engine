{
  "name": "pico-engine",
  "private": true,
  "license": "MIT",
  "scripts": {
    "relock": "npm run rm-nm-and-lock && lerna exec -- npm i",
    "rm-nm-and-lock": "lerna clean && lerna exec -- rm package-lock.json",
    "out": "lerna exec --bail=false -- npm out",
    "up": "lerna exec -- npm up",
    "clean": "lerna clean",
    "bootstrap": "lerna bootstrap --hoist",
    "setup": "npm i && npm run bootstrap",
    "publish": "lerna publish",
    "start": "cd packages/pico-engine && npm start",
    "test": "lerna run test"
  },
  "devDependencies": {
    "lerna": "^2.2.0"
  }
}
