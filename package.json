{
  "private": true,
  "name": "pico-engine-node-prototype",
  "version": "0.0.0",
  "description": "A prototype implementation of the pico-engine written in node.js",
  "main": "src/index.js",
  "scripts": {
    "start": "scriptsp watch:http watch:test",
    "watch:http": "onchange -i src/ -- node src/server.js",
    "watch:test": "onchange -i src/ -- npm run test -s",
    "lint": "eslint src/ && echo lint-ok",
    "test": "npm run lint && node src/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farskipper/pico-engine-node-prototype.git"
  },
  "keywords": [
    "pico",
    "KRE",
    "IoT"
  ],
  "author": "farskipper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/farskipper/pico-engine-node-prototype/issues"
  },
  "homepage": "https://github.com/farskipper/pico-engine-node-prototype#readme",
  "devDependencies": {
    "eslint": "^2.10.2",
    "memdown": "^1.1.2",
    "onchange": "^2.4.0",
    "scriptsp": "^0.3.0",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "bytewise": "^1.1.0",
    "contra": "^1.9.1",
    "cuid": "^1.3.8",
    "http-hash-router": "^1.1.0",
    "leveldown": "^1.4.6",
    "levelup": "^1.3.2",
    "lodash": "^4.11.2"
  }
}
