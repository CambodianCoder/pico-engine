{
  "name": "pico-engine",
  "version": "0.2.0",
  "description": "An implementation of the pico-engine targetting node.js",
  "main": "src/index.js",
  "bin": {
    "pico-engine": "bin/pico-engine"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "start": "scriptsp watch:http watch:test",
    "watch:http": "onchange -i src/ test-rulesets/ -- node src/server/index.js",
    "watch:test": "onchange -i src/ test-rulesets/ -- npm run test -s",
    "lint": "eslint src/ && echo lint-ok",
    "test": "npm run lint && node src/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Picolab/node-pico-engine.git"
  },
  "keywords": [
    "pico-engine",
    "krl",
    "KRE",
    "IoT"
  ],
  "author": "Picolab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Picolab/node-pico-engine/issues"
  },
  "homepage": "https://github.com/Picolab/node-pico-engine#readme",
  "devDependencies": {
    "eslint": "^2.10.2",
    "memdown": "^1.1.2",
    "onchange": "^2.4.0",
    "scriptsp": "^0.3.0",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "bytewise": "^1.1.0",
    "contra": "^1.9.1",
    "cuid": "^1.3.8",
    "fibers": "^1.0.13",
    "home-dir": "^1.0.0",
    "http-hash-router": "^1.1.0",
    "krl-compiler": "^0.10.1",
    "krl-parser": "^0.11.0",
    "krl-stdlib": "^0.1.1",
    "leveldown": "^1.4.6",
    "levelup": "^1.3.2",
    "lodash": "^4.11.2",
    "mkdirp": "^0.5.1",
    "symbol-table": "^1.1.0"
  }
}
